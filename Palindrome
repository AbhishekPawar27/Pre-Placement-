#include<stdio.h>
#include<conio.h>
void main()
{
	char str[100];
	int i,j,n,c=0;
	printf("Enter the string=");
	gets(str);
	n=strlen(str);
	for(i=0;i<=n/2;i++)
	{
		if(str[i]==str[n-i-1])
		c++;
	}
	if(c==i)
	printf("string is palindrome");
	else
	printf("string is not palindrome");
	getch();
}

OUTPUT-
Enter the string=ABBA
string is palindrome

Enter the string=ABBABAB
string is not palindrome
